doctype html
html(ng-app="app")
  head
    meta(name="description" content="{{page.description}}")
    meta(property="og:description" content="{{page.description}}")
    title {{page.tabTitle}}
    meta(http-equiv='content-type' content='text/html; charset=utf-8')
    meta(name='keywords' content='')
    include ../../cms/templates/front-end/mb-links
    script(src="/themes/html5up-txt/controllers/pageCtrl.js")
    script(src="/themes/html5up-txt/routes/routes.js")
    //if lte IE 8
      script(src='/themes/html5up-txt/css/ie/html5shiv.js')
    script(src='/themes/html5up-txt/js/jquery.min.js')
    script(src='/themes/html5up-txt/js/jquery.dropotron.min.js')
    script(src='/themes/html5up-txt/js/skel.min.js')
    script(src='/themes/html5up-txt/js/skel-layers.min.js')
    script(src='/themes/html5up-txt/js/init.js')
    link(rel='stylesheet' href='/themes/html5up-txt/css/skel.css')
    link(rel='stylesheet' href='/themes/html5up-txt/css/style.css')
    link(rel='stylesheet' href='/themes/html5up-txt/css/style-desktop.css')
    //if lte IE 8
      link(rel='stylesheet' href='/themes/html5up-txt/css/ie/v8.css')
  body.homepage(ng-controller="AppCtrl" ng-init="serverData=#{JSON.stringify(serverData)}")
    include ../../cms/templates/front-end/mb-headbar
    div(ng-view)

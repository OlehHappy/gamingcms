
header#header
  .logo.container
    div
      h1
        a#logo(href='index') TXT
      p#description.mb-editable(ng-bind-html="page.content['description'] ? page.content['description'] : defaultText")
//
   Nav 
nav#nav.skel-layers-fixed.mb-fixed-top
  ul(data-menu="main" ng-sortable="mb.sortableMenus")
    li(ng-repeat="menu in menus.main track by menu.id" mb-menu="menu" ng-class="active(menu.url, 'active')")
      a(href="{{menu.url ? menu.url : /}}" target="{{menu.target}}").mb-editable.mb-main-list-a {{menu.title}}
    li
      a(href='#') Dropdown
      ul(data-menu="footer" ng-sortable="mb.sortableMenus").dropdown-menu
        li(ng-repeat="menu in menus.footer track by menu.id" mb-menu="menu" ng-class="active(menu.url, 'active')").mb-draggable
          a(href="{{menu.url ? menu.url: /}}" target="{{menu.target}}").mb-editable.mb-footer-list-a {{menu.title}}
//
   Banner 
#banner-wrapper
  section#banner
    h2#page-title.mb-editable {{page.title}}
    p#content-1.mb-editable(ng-bind-html="page.content['content-1'] ? page.content['content-1'] : defaultText")
    a.button(href='#') Alright let's go
